//method overloading
import java.util.*;
class calculate {
    public int subtract(int n1, int n2){
        int r = n1- n2;
        return r;
    }
    public int subtract(int n1, int n2, int n3){
        int r = n1- n2 - n3;
        return r;
    }
}
public class LAB3_3 {
    public static void main(String a[]) {
        Scanner sc= new Scanner(System.in);
        int num1=14;
        int num2=21;
        int num3=56;
        calculate cal= new calculate();
        int result = cal.subtract(num1,num2,num3);
        int result1 = cal.subtract(num1, num2);
        System.out.println("Result"+result1);
        System.out.println("Result"+result);


    }
}
